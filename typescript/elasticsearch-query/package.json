{
  "name": "elasticsearch-query",
  "version": "0.0.0",
  "main": "index.js",
  "types": "index.d.ts",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "index-mock-data": "curl -H \"Content-Type: application/x-ndjson\" -XPOST -u \"elastic:$(kubectl get secret elasticsearch-query-elasticsearch-e70e9b6b-es-elastic-user -o go-template='{{.data.elastic | base64decode}}')\" \"http://127.0.0.1:9200/cdk/library/_bulk\" --data-binary @sample-data.txt",
    "import": "cdk8s import",
    "compile": "tsc",
    "build": "npm run import && npm run compile && npm run test:update && npm run synth",
    "watch": "tsc -w",
    "synth": "cdk8s synth",
    "test": "jest",
    "test:update": "jest -u"
  },
  "dependencies": {
    "cdk8s": "^2.7.48",
    "constructs": "^10.1.295",
    "cdk8s-plus-26": "^2.2.16"
  },
  "devDependencies": {
    "cdk8s-cli": "^2.2.5",
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "typescript": "^4.9.5",
    "ts-node": "^10.9.1",
    "ts-jest": "^29.0.5"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/**/*(*.)@(spec|test).ts?(x)"
    ],
    "clearMocks": true,
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "watchPathIgnorePatterns": [
      "/node_modules/"
    ],
    "preset": "ts-jest",
    "transform": {
      "\\.[t]s?$": ["ts-jest", { "tsconfig": "tsconfig.dev.json" }]
    }
  }
}
